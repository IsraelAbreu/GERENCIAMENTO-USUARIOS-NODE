<table class="table">
    <thead>
        <tr>
            <th scope="col" class="text-center">#Código</th>
            <th scope="col">Nome</th>
            <th scope="col">Email</th>
            <th scope="col" class="text-center">Permissão</th>
        </tr>
    </thead>
    <tbody>
        <% usuarios.forEach(usuario => { %>
            <tr>
                <th scope="row" class="text-center"><%- usuario.id %></th>
                <td><%- usuario.nome %></td>
                <td><%- usuario.email %></td>
                <td class="text-center"><%- usuario.id_permissao %></td>
            </tr>
        <% }) %>      
    </tbody>
</table>


